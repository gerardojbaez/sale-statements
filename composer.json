{
    "name": "gerardojbaez/sale-statements",
    "description": "A payment-agnostic sales records system for your Laravel application.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerardo Baez",
            "email": "g@gerardobaez.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/database": "~5.7.0|~5.8.0|^6.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Gerardojbaez\\SaleStatements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerardojbaez\\SaleStatements\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gerardojbaez\\SaleStatements\\SaleStatementsServiceProvider"
            ]
        }
    }
}
